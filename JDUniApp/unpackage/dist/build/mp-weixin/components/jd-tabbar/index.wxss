.uni-tabbar{position:fixed;bottom:0;left:0;z-index:999;width:100%;background-color:#fff;box-shadow:0 0 17rpx 1rpx hsla(0,0%,80.8%,.32)}.uni-tabbar .content{display:flex;align-items:flex-end;box-sizing:border-box;height:70px}.uni-tabbar .content .uni-tabbar__item{display:flex;justify-content:center;width:calc((100% - 70px)/4);height:50px}.uni-tabbar .content .uni-tabbar__item .uni-tabbar__icon{width:50px;height:100%}.uni-tabbar .content .uni-tabbar__item .uni-tabbar__icon .icon-img{width:100%;height:100%}.uni-tabbar .content .uni-tabbar__item:nth-child(3){width:70px;height:70px}.uni-tabbar .content .uni-tabbar__item:nth-child(3) .uni-tabbar__icon{width:70px;height:100%}